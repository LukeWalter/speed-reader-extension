{
    "manifest_version": 3,
    "name": "Speed Reader",
    "description": "A chrome extension to make it easier to read long passages of text on websites.",
    "version": "1.0",
    "icons": {
        "16": "/images/icon16.png",
        "48": "/images/icon48.png",
        "128": "/images/icon128.png"
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["embolden.js"]
    }],
    "host_permissions": ["<all_urls>"]
}