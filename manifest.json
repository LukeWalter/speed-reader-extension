{
    "manifest_version": 2,
    "name": "Speed Reader",
    "description": "A chrome extension to make it easier to read long passages of text on websites.",
    "version": "1.0",
    "icons": {
        "16": "/images/logo16.png",
        "48": "/images/logo48.png",
        "128": "/images/logo128.png"
    },
    "page_action": {
        "default_icon": {
            "16": "/images/icon16.png",
            "48": "/images/icon48.png",
            "128": "/images/icon128.png"
        }
    }
}